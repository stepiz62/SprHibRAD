            <method name="open"><![CDATA[importPackage(Packages.org.springframework.context);
importPackage(Packages.org.springframework.web.context.support );
//ServletContext
var sc = reportContext.getHttpServletRequest().getSession().getServletContext();
//ApplicationContext 
var spring = WebApplicationContextUtils.getWebApplicationContext(sc);
var patReps = spring.getBean("patientReports");
var criteria = [params["lastname"]];
var criteria_op = [params["lastname_op"]];
vars["dataIterator"] = patReps.list(criteria,criteria_op,params["orders"],params["orientations"]).iterator();
]]></method>
            <method name="fetch"><![CDATA[var iterator = vars["dataIterator"];
if (iterator.hasNext()) {
	var object = iterator.next();
	row["Currency1"] = object.getCurrency1();
	row["DateOfBirth"] = object.getDateofbirth();
	row["FirstName"] = object.getFirstname();
	row["LastName"] = object.getLastname();
	row["Imagepreview"] = object.getImagepreview();
	return true;
	}
else {
	return false;
}
]]></method>
